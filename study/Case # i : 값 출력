#include <iostream>
#include <vector>
using namespace std;

int main() {
	
	int a, b;
	int t;
	vector<int> arr;

	cin >> t;

	for (int i = 0; i < t; i++) {
		cin >> a >> b;
		arr.push_back(a + b);
	}
	
	for (int i = 0; i < t; i++) {
		cout << "Case #"<< i+1 << ": "<< arr[i] << "\n";
	}



	return 0;
}