#include <iostream>

using namespace std;

int main() {
	int a;
	int b;
	int qu[3];

	cin >> a >> b;

	qu[0] = b % 10; // 385 % 10 나머지 -> 5
	qu[1] = (b / 10) % 10; // 38 % 10 나머지 -> 8
	qu[2] = b / 100; // 3

	for (int i = 0; i < 3; i++)
	{
		cout << a * qu[i] << endl;
	}

	int result = a * b;
	cout << result << endl;


	return 0;
}