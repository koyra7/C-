#include <iostream>

using namespace std;

int main() {

	string N;
	
	cin >> N;


		if (N == "NLCS") {
			cout << "North London Collegiate School";
		}
		else if (N	== "BHA") {
			cout << "Branksome Hall Asia";
		}
		else if (N == "KIS") {
			cout << "Korea International School";
		}
		else if (N == "SJA") {
			cout << "St. Johnsbury Academy";
		}	

	return 0;
}