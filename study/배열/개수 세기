#include <iostream>
#include <vector>
using namespace std;

int main() {

	int number,count;
	vector<int> arr;
	int a;
	int b = 0;

	cin >> count;

	for (int i = 0; i < count; i++) {
		cin >> number;
		arr.push_back(number);
	}

	cin >> a;
	
	for (int i = 0; i < count; i++) {
		if (arr[i] == a) {
			b += 1;
		}
	}


	cout << b;
	
	return 0;
}