#include <iostream>
#include <string>
#include <algorithm>

using namespace std;

int main() {

	string Eng;
	cin >> Eng;

	int num = 0;
	for (char ch : Eng)
		if (ch == 'A' || ch == 'B' || ch == 'C')
			num += 3;
		else if (ch == 'D' || ch == 'F' || ch == 'E')
			num += 4;
		else if (ch == 'G' || ch == 'H' || ch == 'I')
			num += 5;
		else if (ch == 'J' || ch == 'K' || ch == 'L')
			num += 6;
		else if (ch == 'M' || ch == 'N' || ch == 'O')
			num += 7;
		else if (ch == 'P' || ch == 'Q' || ch == 'R' || ch == 'S')
			num += 8;
		else if (ch == 'T' || ch == 'U' || ch == 'V')
			num += 9;
		else if (ch == 'W' || ch == 'X' || ch == 'Y' || ch == 'Z')
			num += 10;

	cout << num;

	return 0;
}