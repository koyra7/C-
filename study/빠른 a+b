#include <iostream>
#include <vector>
using namespace std;

int main() {
	ios::sync_with_stdio(false); //  c언어 타입의 studio 연결 해제 [ 속도 향상 ]
	cin.tie(NULL); //불필요한 flush 방지 [ 속도 향상 ]


	int a, b;
	int t;
	vector<int> arr;

	cin >> t;

	for (int i = 0; i < t; i++) {
		cin >> a >> b;
		arr.push_back(a + b);
	}
	
	for (int i = 0; i < t; i++) {
		cout << arr[i] << '\n';
	}
	

	return 0;
}