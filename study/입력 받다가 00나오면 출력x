#include <iostream>
#include <vector>
using namespace std;

int main() {

	vector<int> arr;
	int a, b;


	while (true) {
		cin >> a >> b;
		arr.push_back(a + b);

		if (a == 0 || b == 0) {
			break;
		}
	}

	for (int i = 0; i < arr.size(); i++) {
		if (arr[i] != 0) {
			cout << arr[i] << '\n';
		}
		else break;
	}
	
	return 0;
}